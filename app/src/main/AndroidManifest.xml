<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.datasiswabimbel">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_round"
        android:label="Admin Bimbel"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.datasiswabimbel.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.datasiswabimbel.addNewStudent"
            android:label="Tambah Data Siswa Baru" />
        <activity
            android:name="com.example.datasiswabimbel.DetailsStudent"
            android:label="Detail Data Siswa" />
        <activity
            android:name="com.example.datasiswabimbel.EditStudent"
            android:label="Edit Data Siswa" />
    </application>

</manifest>